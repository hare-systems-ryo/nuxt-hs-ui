{
  "name": "nuxt-hs-ui",
  "version": "1.0.9",
  "description": "Nuxt UI Pack",
  "repository": "https://github.com/hare-systems-ryo/nuxt-hs-ui",
  "license": "MIT",
  "author": {
    "name": "Ryo@Hare-Systems",
    "email": "admin@hare-systems.net"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./lib/com": {
      "types": "./dist/runtime/lib/com.d.ts",
      "import": "./dist/runtime/lib/com.js",
      "require": "./dist/runtime/lib/com.js"
    },
    "./lib/number": {
      "types": "./dist/runtime/lib/number.d.ts",
      "import": "./dist/runtime/lib/number.js",
      "require": "./dist/runtime/lib/number.js"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "volta": {
    "node": "20.13.0",
    "npm": "10.5.2"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.11.0",
    "@vueuse/integrations": "^10.11.0",
    "bignumber.js": "^9.1.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "dayjs": "^1.11.11",
    "defu": "^6.1.4",
    "focus-trap": "^7.5.4",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.7.1",
    "@nuxt/schema": "^3.12.1",
    "@nuxt/test-utils": "^3.13.1",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/node": "^20.14.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.12.1",
    "sass": "^1.77.5",
    "tailwind-merge": "^2.3.0",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "overrides": {
    "vue": "latest"
  }
}
